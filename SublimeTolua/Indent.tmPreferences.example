<?xml version="1.0" encoding="UTF-8"?>
<plist version="1.0">
<dict>
    <key>name</key>
    <string>Indent</string>
    <key>scope</key>
    <string>source.lua</string>
    <key>settings</key>
    <dict>
        <key>decreaseIndentPattern</key>
        <string>^\s*(else|end|\}|end\))\s*$|^\s*elseif\b((?!\bend\b).)*$</string>
        <key>increaseIndentPattern</key>
        <string>^\s*(else|elseif|for|if|repeat|until|while)\b((?!\bend\b).)*$|.*\{\s*$|^.*\bfunction\b((?!\bend\b).)*$</string>
    </dict>
</dict>
</plist>


